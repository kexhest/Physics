{
  "name": "physics",
  "version": "1.0.0",
  "description": "Port of the Traer Physics Library from Processing.",
  "homepage": "http://jonobr1.github.io/Physics/",
  "author": "Jono Brandel (http://jonobr1.com)",
  "contributors": [
    "Magnus Bergman <hello@magnus.sexy> (https://magnus.sexy)"
  ],
  "keywords": [
    "physics",
    "traer"
  ],
  "engines": {
    "node": "5.0.0",
    "npm": "3.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:magnus-bergman/Physics.git"
  },
  "main": "./lib/index.js",
  "scripts": {
    "test": "NODE_ENV=test karma start karma.conf.js --log-level debug --single-run",
    "lint": "standard",
    "clean": "rimraf build",
    "prebuild": "npm run clean -s",
    "build": "npm-run-all build:dev build:prod",
    "build:dev": "NODE_ENV=dev webpack -p --colors --progress",
    "build:prod": "NODE_ENV=production webpack -p --colors --progress",
    "compile": "babel -d lib/ src/",
    "prepublish": "npm run compile"
  },
  "files": [
    "lib"
  ],
  "standard": {
    "parser": "babel-eslint"
  },
  "dependencies": {
    "babel-cli": "^6.1.18",
    "babel-core": "^6.1.21",
    "babel-eslint": "^4.1.5",
    "babel-loader": "^6.1.0",
    "babel-preset-es2015": "^6.1.18",
    "karma": "^0.13.15",
    "karma-chrome-launcher": "^0.2.1",
    "karma-cli": "0.1.1",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.1",
    "karma-nyan-reporter": "0.2.2",
    "karma-sinon-chai": "^1.1.0",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.4",
    "npm-run-all": "^1.2.13",
    "object-assign": "^4.0.1",
    "raf": "^3.1.0",
    "rimraf": "^2.4.3",
    "standard": "^5.4.0",
    "webpack": "^1.12.6"
  },
  "devDependencies": {},
  "license": "MIT"
}
